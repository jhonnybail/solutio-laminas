{
    "name": "solutio/module-laminas",
    "description": "Laminas library development by Solutio.Me",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "orm",
        "module",
        "zf",
        "solutio"
    ],
    "authors": [
        {
            "name": "Jonathan Bogdanovicz",
            "email": "jhonnybail@gmail.com"
        }
    ],
    "extra": {
        "laminas": {
            "module": "Solutio"
        }
    },
    "require": {
        "php": "^7 || ^8",
        "doctrine/doctrine-orm-module": "^6.0.0",
        "laminas/laminas-server": "^2.8.1",
        "laminas/laminas-mvc": "^3.1.1",
        "laminas/laminas-servicemanager": "^3.4.0",
        "laminas/laminas-log": "^2.12.0",
        "laminas/laminas-cache": "^3.6.0",
        "laminas/laminas-session": "^2.9.1",
        "laminas/laminas-json": "^3.1.2",
        "php-curl-class/php-curl-class": "^7.4.0",
        "acelaya/zf2-acmailer": "^8.2.0",
        "ramsey/uuid": "^3.9.3",
        "laminas/laminas-dependency-plugin": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Solutio\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    }
}
